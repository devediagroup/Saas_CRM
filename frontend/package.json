  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "NODE_ENV=production vite build",
    "build:analyze": "npm run build -- --mode analyze",
    "build:stats": "vite build --mode production && npx vite-bundle-analyzer dist/stats.html",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "preview:prod": "npm run build:prod && vite preview --port 3000 --host",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ui": "jest --watch --verbose",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:headed": "cypress run --headed",
    "cypress:run:record": "cypress run --record",
    "e2e": "cypress run",
    "e2e:open": "cypress open",
    "e2e:headed": "cypress run --headed",
    "e2e:chrome": "cypress run --browser chrome",
    "e2e:firefox": "cypress run --browser firefox",
    "e2e:auth": "cypress run --spec 'cypress/e2e/auth.cy.ts'",
    "e2e:leads": "cypress run --spec 'cypress/e2e/leads-simple.cy.ts'",
    "e2e:properties": "cypress run --spec 'cypress/e2e/properties.cy.ts'",
    "e2e:dashboard": "cypress run --spec 'cypress/e2e/dashboard.cy.ts'",
    "test:e2e": "start-server-and-test dev http://localhost:8080 e2e",
    "test:e2e:ci": "start-server-and-test dev http://localhost:8080 'cypress run --record'",
    "performance:audit": "npx lighthouse http://localhost:3000 --output=json --output=html --output-path=./lighthouse-report.json",
    "performance:ci": "lhci autorun",
    "bundle:size": "npx vite-bundle-analyzer dist/stats.html",
    "bundle:optimize": "npm run build:prod && npm run bundle:size",
    "pwa:generate": "npx vite build && npx workbox generateSW",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules node_modules/.vite package-lock.json",
    "cache:clear": "rm -rf node_modules/.vite dist",
    "postinstall": "npm run cache:clear",
    "docker:build": "docker build -t echoops-frontend .",
    "docker:run": "docker run -p 3000:3000 echoops-frontend",
    "docker:dev": "docker-compose up frontend --build",
    "start": "npm run dev",
    "serve": "npm run preview"
  },
